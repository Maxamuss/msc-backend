{
    "list": {
        "page_object_fields": [],
        "layout": [
            {
                "component": "header",
                "config": {
                    "title": "Functions",
                    "subtitle": "Function that you have defined to use in your application.",
                    "tools": [
                        {
                            "component": "button",
                            "config": {
                                "text": "Add Function",
                                "uri": "${model}:create",
                                "icon": "ViewGridAddIcon"
                            }
                        }
                    ]
                }
            },
            {
                "component": "table",
                "config": {
                    "model": "function",
                    "fields": [
                        {
                            "field_name": "function_name",
                            "verbose_name": "Function Name",
                            "sortable": true,
                            "default_sorting": null
                        }
                    ]
                }
            }
        ]
    },
    "create": {
        "page_object_fields": [],
        "layout": [
            {
                "component": "header",
                "config": {
                    "title_text": "Create Function",
                    "subtitle_text": null,
                    "tools": []
                }
            },
            {
                "component": "form",
                "config": {
                    "model": "function",
                    "uri": "function:edit:${id}",
                    "fields": [
                        {
                            "field_name": "function_name",
                            "label": "Function Name",
                            "widget": "TextField",
                            "required": true,
                            "default": null,
                            "help_text": null,
                            "placeholder": null
                        }
                    ],
                    "save_button_text": "Create Function"
                }
            }
        ]
    },
    "edit": {
        "page_object_fields": [
            "function_name",
            "id"
        ],
        "layout": [
            {
                "component": "header",
                "config": {
                    "title_text": "{{ function_name }}",
                    "subtitle_text": "{{ id }}",
                    "tools": [
                        {
                            "component": "button",
                            "config": {
                                "text": "Delete Function",
                                "icon": "ViewGridAddIcon",
                                "link_uri": "function:delete:${id}"
                            }
                        }
                    ]
                }
            },
            {
                "component": "form",
                "config": {
                    "model": "function",
                    "action": "patch",
                    "fields": [
                        {
                            "field_name": "function_name",
                            "label": "Function Name",
                            "widget": "TextField",
                            "required": true,
                            "default": null,
                            "help_text": null,
                            "placeholder": null
                        }
                    ]
                }
            }
        ]
    },
    "delete": {
        "page_object_fields": [
            "function_name",
            "id"
        ],
        "layout": [
            {
                "component": "header",
                "config": {
                    "title_text": "Delete Function: {{ function_name }}",
                    "subtitle_text": "{{ id }}",
                    "tools": []
                }
            }
        ]
    }
}